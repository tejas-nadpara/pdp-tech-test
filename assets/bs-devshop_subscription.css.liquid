
@font-face {
    font-family: 'handelgothic_btregular';
    src: url('handel-gothic-bt-webfont-webfont.eot');
    src: url('handel-gothic-bt-webfont-webfont.eot?#iefix') format('embedded-opentype'),
         url('handel-gothic-bt-webfont-webfont.woff2') format('woff2'),
         url('handel-gothic-bt-webfont-webfont.woff') format('woff'),
         url('//cdn.shopify.com/s/files/1/2031/9099/files/handel-gothic-bt-webfont-webfont.ttf') format('truetype'),
         url('handel-gothic-bt-webfont-webfont.svg#handelgothic_btregular') format('svg');
    font-weight: normal;
    font-style: normal;

}
.bs-devshop__family{ font-family: 'handelgothic_btregular';  font-weight: normal;}


.bs-devshop__flex{ display: -webkit-box;  display: -moz-box;  display: -ms-flexbox;  display: -webkit-flex;  display: flex;}
.bs-devshop__flex-flow{-webkit-flex-flow: row wrap;	 flex-flow: row wrap;   -ms-flex-flow: row wrap;}
.bs-devshop__align-item-start{-webkit-box-align: flex-start;  -moz-box-align: flex-start;  -ms-flex-align: flex-start;  -webkit-align-items: flex-start;  align-items: flex-start;}
.bs-devshop__align-item-center{-webkit-box-align: center;  -moz-box-align: center;  -ms-flex-align: center;  -webkit-align-items: center;  align-items: center;}
.bs-devshop__align-item-end{-webkit-box-align: end;  -moz-box-align: end;  -ms-flex-align:end;  -webkit-align-items:end;  align-items:end;}
.bs-devshop__space-between{  -webkit-box-pack: space-between;  -moz-box-pack: space-between;  -ms-flex-pack: space-between;  -webkit-justify-content: space-between;  justify-content: space-between;}
.bs-devshop__justify-center{  -webkit-box-pack: center;  -moz-box-pack: center;  -ms-flex-pack: center;  -webkit-justify-content: center;  justify-content: center; }
.bs-devshop__justify-center-end{  -webkit-box-pack: end;  -moz-box-pack: end;  -ms-flex-pack: end;  -webkit-justify-content: end;  justify-content: end; }
.bs-devshop__flex-direction-column{ -webkit-flex-direction: column;   -moz-flex-direction: column;    -ms-flex-direction: column;    -o-flex-direction: column;    flex-direction:Â column;}


.bs-devshop__subscription-cover{ border-radius:10px;     overflow: hidden; background:var(--orange-box-bg); border: var(--orange-box-border) solid 3px;}
.bs-devshop__subscription-cover h5{margin:0; padding:22px 10px 12px; text-align:center; font-size: 2.4rem;line-height: 28.77px; letter-spacing: 0;}


.bs-devshop__form_label {
    display: block;
    text-align: center;
    margin: 0;
   font-size: 1.8rem;
   font-weight: 400;
  text-shadow: 3px 3px 5.5px 0px #FF000066;
   background: linear-gradient(90deg, #FF0000 0%, #FFB800 18%, #FF6B00 38.5%, #FF0000 59.5%, #FFB800 79.5%, #FF6B00 100%);
   -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;     

}
.bs-devshop__swatch_cover {    margin: 11px 0 0;}
.product-form__input.product-form__input--pill {    margin: 0;    padding: 0 8px;}
.bs-devshop__swatch_box {width:33.33%; padding:0 6px;}
.product-form__input--pill .bs-devshop__swatch_box input[type=radio]+label {    padding:15px 10px 6px;  width: 100%;    border:var(--grey-border-light)  solid 2px; background: var(--grey-bg); letter-spacing: 0;margin: 0;    border-radius: 10px;  color: var(--grey-color);}
.product-form__input--pill .bs-devshop__swatch_box input[type=radio]:checked+label {    background-color:var(--orange-light-bg);    color: var(--black);   border-color:var(--orange-box-border);}
.product-form__input--pill .bs-devshop__swatch_box input[type=radio]:checked+label .bs-devshop__oldPrice{background: var(--red); color:var(--white);}
.bs-devshop__oldPrice {
    font-size: 1.2rem;
    position: absolute;
    top: -12px;
    left: 0;
    right: 0;
    border-radius: 2rem;
    background: var(--grey-bg);
    color: var(--grey-color);
    line-height: 1.4rem;
    width: 98%;
    margin: 0 auto;
    padding: 3px 4px;
    line-height: normal;
}
.bs-devshop__qty{font-size:2rem;     padding: 0 0 2px;}
.bs-devshop__save-price {   border-radius: 2px;    font-size: 1rem;    display: inline-block;    padding: 3px 5px;}
.product-form__input--pill .bs-devshop__swatch_box input[type=radio]:checked+label .bs-devshop__save-price{ color: var(--white);     background: var(--green);}
.bs-devshop__button{padding:0 14px;}
.bs-devshop__button .product-form{margin:0; padding:0;}

.bs-devshop__delivery ul {    margin: 0;    padding:0 0 10px;    list-style: none;}
.bs-devshop__delivery ul li{ border-radius:2px;   margin: 0;    padding:2px 8px;    list-style: none; letter-spacing:0;font-size: 1.5rem;font-weight:500;line-height: 15.85px;}
.bs-devshop__delivery ul li:first-child{background:var(--blue); color:var(--white); font-weight: 700; letter-spacing: 0.1em; font-size: 1.3rem;}
.bs-devshop__delivery ul li svg {    display: block;    line-height: normal;    position: relative;    margin-right: 5px;    top: -2px;}

.bs-devshop__addcart{ padding-left:5px; padding-right:5px; font-size: 2.4rem; background: var(--orange-btn-background); border:var(--orange-btn-border) solid 2px; color: var(--black); border-radius: 10px;  letter-spacing: 0;   overflow: hidden; margin: 0;     min-height: 60px;}
.bs-devshop__addcart:after, .bs-devshop__addcart:before {display:none;}
.bs-devshop__addcart svg{    position: relative;     top: 4px;}

.bs-devshop__guarantee ul{ margin: 0;    padding:5px 0;    list-style: none;}
.bs-devshop__guarantee ul li { margin: 0;    padding:0px 4px; letter-spacing: 0;   list-style: none;font-size: 1.3rem;font-weight: 500;line-height: 15.85px;}
.bs-devshop__guarantee ul li img{margin-right:3px;}

.bs-devshop__payment-part ul{ margin:40px 0 0;    padding:0;    list-style: none; background: var(--orange-light-bg); }
.bs-devshop__payment-part ul li{ margin: 0;     padding:25px 5px;  text-align: center;     list-style: none; width:50%; font-weight: 600; font-size:1.3rem; line-height: 15.85px;}
.bs-devshop__payment-part ul li:first-child{ background: var(--orange-light-bg-2);}
.bs-devshop__payment-icon {    text-align: center;    line-height: normal;}
.bs-devshop__payment-part ul li .lock{width:30px; display:block;}
.bs-devshop__product-subscription{padding:0 14px 35px; }
.bs-devshop__product-subscription ul{margin:0; padding:0; list-style: none; position: relative;}
.bs-devshop__product-subscription ul li{margin:0; padding:0; list-style: none; cursor: pointer;}
.bs-devshop__one-time{color: var(--grey-color); background:var(--grey-bg); font-size: 2.0rem;line-height: 23.97px; border-radius:10px 10px 0 0; border-width: 2px 2px 0px 2px; border-style: solid;border-color: var(--grey-border-light);     padding: 15px 10px;}
.bs-devshop__option-left {    position: relative;    padding-left: 30px; padding-right:25px;}
.bs-devshop__option-right {
    text-align: right;
    white-space: nowrap; font-family: var(--font-body-family);
  font-weight:700;
  font-size:2.4rem;
} 
.bs-devshop__option-left:after{content:""; width:22px; height:22px; border-radius:50%; border:var(--grey-color) solid 1px;}
.bs-devshop__option-left:after, .bs-devshop__option-left:before {    content: "";    width: 22px;    height: 22px;    border-radius: 50%;    border: var(--grey-color) solid 1px;    position: absolute;    left: 0;    top: 0;}
.bs-devshop__option-left:before {  opacity:0;  background: #6c6c6c;    width: 18px;    height: 18px;    top: 2px;    left: 2px;}
.bs-devshop__product-subscription ul li.active .bs-devshop__option-left:before{opacity:1; background: var(--black); border: var(--black) solid 1px; }

.bs-devshop__subscribe{color: var(--black); background: var(--grey-bg);  font-size: 2.0rem;line-height: 23.97px; border-radius:0 0 10px 10px ; border-width: 0px 2px 2px 2px; border-style: solid;border-color:var(--orange-btn-background);     padding: 10px 10px; }
.bs-devshop__product-subscription ul li.active .bs-devshop__one-time, .bs-devshop__product-subscription ul li.active .bs-devshop__subscribe{background: var(--orange-light-bg);}

.bs-devshop__option-left font{color:var(--orange-btn-background);}
.bs-devshop__subscribe_price {
    color: var(--red);
    display: block;
    padding-top: 6px;
}
.bs-devshop__option-left span{font-size: 1.1rem; display:block; font-weight: 500;line-height: 13.41px;     font-family: var(--font-body-family);}
.bs-devshop__option-price-cover {font-size: 1rem;  font-family: var(--font-body-family);font-weight: 500;line-height: 12.19px;}                    
.bs-devshop__option-price-cover span {
    background: var(--orange-btn-background);

    text-align: center;
    white-space: nowrap;
    padding: 0px 2px;
    display: inline-block;
    margin: 0 3px 0 0;
    line-height: normal;
}
.bs-devshop__option-right p{margin:0; padding:0; font-family: var(--font-body-family); 
font-size: 1.1rem;
font-weight: 500;
line-height: 13.41px;

}

.bs-devshop__shadow {
    background: linear-gradient(89.85deg, red .14%, #989b00 20.59%, #007871 39.98%, #02006d 54.73%, #ab00c7 66.97%, #bd0066 78.75%, #ed001c 91.41%, red 99.88%);
    border-radius: 4px 0 0;
    position: absolute;
    width: 100%;
    bottom: 0px;
    height: 90px;
    
   -webkit-filter: blur(10px);
  -moz-filter: blur(10px);
  -o-filter: blur(10px);
  -ms-filter: blur(10px);
  filter: blur(10px);
}
.bs-devshop__relative{position: relative;     z-index: 1;}
.sealsubs-target-element{display:none;}

@media(max-width:445px){
.product-form__input--pill .bs-devshop__swatch_box input[type=radio]+label {    padding: 15px 5px 6px;}
.bs-devshop__qty {    font-size: 1.5rem;}
.bs-devshop__oldPrice{font-size: 1rem;}  
.bs-devshop__swatch_box{padding:0 4px;}  
.bs-devshop__save-price {    font-size: 0.9rem;    padding: 3px 3px;}  
.bs-devshop__subscription-cover h5{font-size: 2rem;}  
.bs-devshop__delivery ul li:first-child {      font-size: 1.1rem;}  
.bs-devshop__option-left{padding-right:10px; padding-left: 28px;}  
 .bs-devshop__subscribe, .bs-devshop__one-time{font-size: 1.6rem;} 
.bs-devshop__option-right {    font-size: 2.1rem;}  
.bs-devshop__guarantee ul li {    padding: 0 2px;    font-size: 1rem;}  
.bs-devshop__payment-part ul li{font-size: 1rem;} 
.bs-devshop__addcart{font-size: 2rem;}  
}

@media(max-width:400px){
.bs-devshop__oldPrice{font-size:0.8rem;}  
}
